#include <string>
#include <vector>
â€‹
std::string range_extraction(std::vector<int> args) {
  std::string ans = "";
  for(int i =0;i<args.size();i++){
    int j = 0;
    if(args[i+1]==args[i]+1 && args[i+2]==args[i+1]+1){
      for(j =0 ; j < args.size() ; j++){
        if(args[i+j]+1!=args[i+j+1]){
          break;
        }
      }
      
      ans += std::to_string(args[i]) +'-' + std::to_string(args[i+j]);
      i+=j;
      if(i!=args.size()-1)
        ans+=',';
        
      
    }
    else{
      if(i==args.size()-1)
      ans += std::to_string(args[i]);
      else
        ans += std::to_string(args[i]) +',';
        
    }
  }
  return ans;
}