#include <cmath>
using namespace std;
int square_digits(int num) {
  int ans =0,n,i=0,ii,j,temp=0,tt;
  ii=to_string(num).length();
  for(i=ii-1;i>=0;i--){
    j=pow(10,i);
    temp = num/j;
    n=temp*temp;
    tt=0;
    if(n==0){
      tt=1;
    }
    else{
    while(n>0){
      n=n/10;
      tt++;
      }
    }
    ans=ans*pow(10,tt) + (temp*temp);
      num=num%j;
  }
 return ans;
}