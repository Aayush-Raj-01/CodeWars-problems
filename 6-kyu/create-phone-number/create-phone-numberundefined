#include <string>
â€‹
std::string createPhoneNumber(const int arr [10]){
  char str[50];
  int j=0;
  for(int i=0;i<=9;i++){
    if(j==0){
      str[j] = '(';
      j++;
    }
    if(j==4){
      str[j] = ')';
      j ++;
    }
    if(j==5){
      str[j] = ' ';
      j ++;
    }
    if(j==9){
      str[j]='-';
      j++;
    }
    str[j]=arr[i] + '0';
    j++;
    
    
    
  }
  str[j]= '\0';
  return str;
}