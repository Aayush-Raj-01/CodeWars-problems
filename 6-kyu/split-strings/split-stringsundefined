#include <string>
#include <vector>
â€‹
std::vector<std::string> solution(const std::string &s)
{
  std::vector<std::string> ve;
  int j = 0;
  for(size_t i = 0 ; i < s.size();i+=2){
    if(i+1== s.size()){
      ve.push_back(std::string() + s[i] + '_');
      break;
    }
    ve.push_back(std::string() + s[i] + s[i+1]);
    j++;
  }
    return ve;
  }