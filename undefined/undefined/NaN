#include <numeric>
class Fracts
{
​
public:
    static std::string convertFrac(std::vector<std::vector<unsigned long long>> &lst){
      std::string ans = "" ;
      long long int res=1;
      for(int i =0; i< lst.size();i++){
        res = std::lcm(res,lst[i][1]);
      }
      for(int i =0; i< lst.size();i++){
      ans += "(" + std::to_string(lst[i][0]*(res/lst[i][1])) + "," + std::to_string(res) + ")";
      }
      return ans;
      
    }
};
